import math as m
def isprime(p):
    sq=int(m.sqrt(p))
    for i in range(2,sq+1):
        if(p%i==0):
            return False
    return True
def semiprime(p):
    sq=int(m.sqrt(p))
    for i in range(2,sq+1):
        if (p%i==0):
            if(isprime(i) and isprime(p//i)):
                return True
    return False
t=int(input("enter number of test cases"))
for i in range(t):
    n=int(input("enter n value"))
    print(semiprime(n))
